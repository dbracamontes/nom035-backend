# Copy this file to `.env` and fill values before running docker compose
# DO NOT commit real secrets to the repository.

# MySQL settings
MYSQL_DATABASE=nom035
MYSQL_ROOT_PASSWORD=change_me_root_password
MYSQL_USER=nom035user
MYSQL_PASSWORD=change_me_user_password

# Spring datasource (optional, will default to jdbc:mysql://mysql:3306/${MYSQL_DATABASE})
SPRING_DATASOURCE_URL=jdbc:mysql://mysql:3306/nom035?useSSL=false&serverTimezone=UTC&allowPublicKeyRetrieval=true
SPRING_DATASOURCE_USERNAME=root
SPRING_DATASOURCE_PASSWORD=change_me_user_password
